{
  "name": "histable",
  "description": "Maintains history of state changes",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "docs": "jsdoc2md --global-index-format grouped index.js > DOCS.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Tushar Mathur <tusharmath@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "semantic-release": "^6.3.6"
  },
  "keywords": [
    "history",
    "immutable",
    "forward",
    "backward",
    "redo",
    "undo"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tusharmath/histable.git"
  },
  "dependencies": {
    "coveralls": "^2.11.9",
    "nyc": "^10.3.2"
  }
}
